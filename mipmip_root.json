{
"mipmip.task.echo": {
	"args": {
		"positional": [
			"mipmip.artifacts.cat_sample_file.stdout",
			"apples", "oranges"
		],
		"named": {
			"--grocery_list": ["apples", "oranges"],
			"--grocery_list_2": {
				"mipmip.glue": "|",
				"mipmip.values": ["apples", "oranges"]
			},
			"--grocery_list_3": {
				"apples": "1",
				"oranges": "2",
				"mipmip.inner_glue": ":",
				"mipmip.glue": "/"
			},
			"--grocery_list_4": {
				"apples": {
					"green": "1",
					"red": "2"
				},
				"oranges": "2",
				"mipmip.inner_glue": ":",
				"mipmip.glue": " ",
				"mipmip.wrap": "\""
			}
		}
	}
},
"mipmip.workflow.echo2": {
	"tasks": [
		"cat_sample_file",
		"echo"
	]
},
"pyramid.include": [
	"mipmip_parent.json"
],
"pyramid.merge_policy": {
	"mipmip.task.echo": {
		"args": {
			"positional": "append"
		}
	}
}
}

